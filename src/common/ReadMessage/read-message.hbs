<div>
    {{#if from}}
        <div class="my_message-position">
            <div class="my_message f-light">
                {{#if this.content}}
                    {{this.content}}
                {{/if}}
                <div class="my_message-time f-mini">
                    <svg
                        width="11"
                        height="5"
                        viewBox="0 0 11 5"
                        fill="none"
                        xmlns="http://www.w3.org/2000/svg"
                    >
                        <line
                            y1="-0.5"
                            x2="3.765"
                            y2="-0.5"
                            transform="matrix(0.705933 0.708278 -0.705933 0.708278 0.700195 2.33313)"
                            stroke="#3369F3"
                        ></line>
                        <line
                            y1="-0.5"
                            x2="5.6475"
                            y2="-0.5"
                            transform="matrix(0.705933 -0.708278 0.705933 0.708278 3.35828 5.00006)"
                            stroke="#3369F3"
                        ></line>
                        <line
                            y1="-0.5"
                            x2="5.6475"
                            y2="-0.5"
                            transform="matrix(0.705933 -0.708278 0.705933 0.708278 6.01587 5.00006)"
                            stroke="#3369F3"
                        ></line>
                    </svg>
                    {{time}}
                </div>
            </div>
        </div>
    {{else}}
        <div class="contact_msg f-light">
            {{#if this.content}}
                {{this.content}}
            {{/if}}
            <div class="contact_msg-time f-mini">
                {{time}}
            </div>
        </div>
    {{/if}}
</div>