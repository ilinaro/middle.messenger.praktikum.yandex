<html lang="ru">
    <main class="PrimaryLayout">
        <div class="PrimaryLayout__content">
            <div class="form_login">
                <div class="f-header">
                    Вход
                </div>
                <form onsubmit="event.preventDefault()">
                    <div class="input_login">
                        {{{Login}}}
                    </div>
                    <div class="input_pass">
                        {{{Password}}}
                    </div>
                    <div class="buttons">
                        {{{ButtonLogin}}}
                        {{{LinkRegistration}}}
                    </div>
                </form>
            </div>
        </div>
    </main>
</html>