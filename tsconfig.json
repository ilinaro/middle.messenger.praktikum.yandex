{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "commonjs",
    "forceConsistentCasingInFileNames": true,
    "declaration": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "es2020",
    "noImplicitAny": true,
    "outDir": "./dist/",
    "strict": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "sourceMap": true
  }
}
